{
  "name": "@my-app/server",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "test": "jest",
    "seed": "ts-node scripts/seed.ts",
    "db:migrate-save": "prisma migrate dev",
    "db:migrate-up": "prisma migrate deploy",
    "db:clean": "ts-node scripts/clean.ts",
    "db:init": "run-s \"db:migrate-save -- --name 'initial version'\" db:migrate-up seed",
    "prisma:generate": "prisma generate",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:build": "docker build .",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down --volumes"
  },
  "dependencies": {
    "@nestjs/common": "9.3.7",
    "@nestjs/config": "2.3.0",
    "@nestjs/core": "9.3.7",
    "@nestjs/graphql": "10.2.0",
    "@nestjs/jwt": "10.0.0",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.3.7",
    "@nestjs/serve-static": "3.0.0",
    "@nestjs/swagger": "6.2.1",
    "@prisma/client": "4.10.1",
    "apollo-server-express": "3.10.2",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "graphql": "16.5.0",
    "graphql-type-json": "0.3.2",
    "nest-access-control": "2.2.0",
    "nest-morgan": "2.0.0",
    "nestjs-prisma": "0.20.0",
    "npm-run-all": "4.1.5",
    "passport": "0.6.0",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "swagger-ui-express": "4.6.0",
    "ts-node": "10.9.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.2.0",
    "@nestjs/testing": "9.3.7",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.13",
    "@types/graphql-type-json": "0.3.2",
    "@types/jest": "28.1.6",
    "@types/normalize-path": "3.0.0",
    "@types/passport-http": "0.3.9",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "jest": "28.1.3",
    "jest-mock-extended": "3.0.1",
    "prisma": "4.10.1",
    "supertest": "6.3.0",
    "ts-jest": "28.0.8",
    "type-fest": "3.0.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}